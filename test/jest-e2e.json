{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": [
      "ts-jest",
      {
        "transpileOnly": true,
        "transpilation": true,
        "useESM": false
      }
    ]
  },
  "forceExit": true,
  "detectOpenHandles": true
}
